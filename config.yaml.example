# https://platform.openai.com/account/api-keys
apiKey:

temperature: 0.2 # lower means stable response, higher means more random
baseURL: https://api.openai.com/v1
useProxy: false
proxyUrl: http://127.0.0.1:6152
maxHistory: 2500

model: gpt-3.5-turbo-0125 # default
modelTokenLimit: 16384 # default
modelTokenThrottle: 0.8 # usable token count: (modelTokenLimits[x] - modelResponseMaxToken) * modelTokenThrottle
models: # https://platform.openai.com/docs/models
  - gpt-3.5-turbo-0125
  - gpt-4-0125-preview
modelResponseMaxToken: 4096 # official value
modelTokenLimits:
  gpt-3.5-turbo-0125: 16385 # gpt-3.5 https://platform.openai.com/docs/models/gpt-3-5-turbo
  gpt-4-0125-preview: 128000 # gpt-4 https://platform.openai.com/docs/models/gpt-4-and-gpt-4-turbo
options:
  optionsAssistant:
    common: You are a helpful assistant.
    translator: You are a translator. You will be provided with a sentence, and your task is to translate it into {LANG}.
  optionsLang:
    - en
    - zh
    - ja
logPromptAndRes: false
langVocal:
  zh: Meijia
  en: Samantha
  ja: Kyoko
